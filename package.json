{
  "name": "@darabonba/typescript-generator",
  "version": "2.0.0",
  "description": "The darabonba generator for TypeScript",
  "main": "lib/generator",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "mocha --reporter spec --timeout 3000 test/*.test.js",
    "test-cov": "nyc -r=lcov -r=html -r=text -r=json mocha -t 3000 -R spec test/*.test.js",
    "ci": "npm run lint && npm run test-cov && codecov",
    "lint": "eslint --fix ./lib ./test"
  },
  "author": "Alibaba Cloud OpenAPI Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aliyun/darabonba-typescript-generator.git"
  },
  "dependencies": {
    "@darabonba/parser": "^2"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "mocha": "^8.1.3",
    "codecov": "^3.8.0",
    "nyc": "^15.1.0"
  },
  "files": [
    "lib"
  ],
  "bugs": {
    "url": "https://github.com/aliyun/darabonba-typescript-generator/issues"
  },
  "homepage": "https://github.com/aliyun/darabonba-typescript-generator#readme"
}
